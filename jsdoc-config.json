{
	"tags": {
		"allowUnknownTags": true
	},
	"sourceType": "module",
	"source": {
		"include": [
			"src",
			"./README.md"
		]
	},
	"plugins": ["plugins/markdown"],
	"templates": {
		"referenceTitle": "OVH Object Storage",
		"cleverLinks": false,
		"collapse": true,
		"monospaceLinks": false,
		"default": {
			"outputSourceFiles": true
		}
	},
	"opts": {
		"encoding": "utf8",
		"destination": "./doc",
		"template": "./node_modules/braintree-jsdoc-template"
	},
	"markdown.hardwrap": true,
	"markdown": {
		"hardwrap": true
	},
	"metadata": {
		"title": "OVH Object Storage"
	}
}
